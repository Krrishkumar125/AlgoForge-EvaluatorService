{
  "name": "evaluator-service",
  "version": "1.0.0",
  "type": "module",
  "description": "This is the evaluator service for the algoforge platform",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "watch": "npx tsc -w",
    "prestart": "npm run build",
    "start": "npx nodemon dist/index.js",
    "dev": "npx concurrently --kill-others \"npm run watch\" \"npm run start\""
  },
  "author": "Krrish",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "nodemon": "^3.1.11"
  }
}
